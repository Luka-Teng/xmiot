{
  "name": "root",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "publish": "node scripts/publish.js",
    "prettier": "prettier-eslint --write \"**/*.jsx\" \"**/*.js\" --eslint-config-path \"./.eslintrc\"",
    "start-prod": "cross-env NODE_ENV=production node scripts/devServer.js",
    "start-dev": "cross-env NODE_ENV=development node scripts/devServer.js",
    "bootstrap": "lerna exec npm i",
    "init": "node scripts/init.js",
    "test": "cross-env NODE_ENV=testing jest --no-cache",
    "debug-test": "node --inspect-brk $(npm bin)/jest --no-cache"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "packages/**/*.js": [
      "prettier-eslint --eslint-config-path \"./.eslintrc\" --write",
      "git add"
    ],
    "packages/**/*.ts": [
      "prettier-eslint --eslint-config-path \"./.eslintrc\" --write",
      "git add"
    ]
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "packages/*/src/**/*.js"
    ],
    "collectCoverage": false,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/packages/$1"
    },
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ],
    "testRegex": "/__tests__/.*\\.test\\.js$",
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "\\.js$": "<rootDir>/tests/jsCodeTransform.js",
      "\\.tsx?$": "<rootDir>/tests/tsCodeTransform.js",
      "^.+\\.css$": "<rootDir>/tests/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/tests/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$",
      "node_modules/[^/]+?/(?!(es|node_modules)/)"
    ],
    "globals": {
      "ts-config": "temp"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-transform-member-expression-literals": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-property-literals": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/plugin-transform-template-literals": "^7.2.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-plugin-inline-import-data-uri": "^1.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.4.1",
    "commander": "^2.19.0",
    "consolidate": "^0.15.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-15": "^1.3.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "fs-extra": "^7.0.0",
    "handlebars": "^4.0.12",
    "husky": "^1.1.2",
    "inquirer": "^6.2.0",
    "jest": "^24.0.0",
    "lerna": "^3.4.3",
    "less": "^3.8.1",
    "lint-staged": "^7.3.0",
    "metalsmith": "^2.3.0",
    "minimatch": "^3.0.4",
    "ora": "^3.0.0",
    "prettier": "1.14.3",
    "prettier-eslint-cli": "^4.7.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-file-as-blob": "^1.1.2",
    "rollup-plugin-less": "^0.1.5",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-serve": "^0.6.0",
    "rollup-plugin-terser": "^3.0.0",
    "rollup-plugin-typescript2": "^0.19.2",
    "semver": "^5.6.0",
    "simple-git": "^1.107.0",
    "ts-jest": "^23.10.5",
    "tslib": "^1.9.3",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "postcss-url": "^8.0.0"
  },
  "lastCommit": "7db14806730ef6966a117bbfe4bfa318dd949eeb"
}
